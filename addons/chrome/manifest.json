{
    "name": "OpenWebApps",
    "version": "0.1",
    "description": "Provides rich browser support for open web applications.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png"
    },
    "background_page": "background.html",
    "permissions": [ ],
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [ "<all_urls>" ],
            "js": [ "inject_api.js" ]
        }
    ]
}
