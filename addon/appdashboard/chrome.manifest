# Register a resource: protocol alias so we can refer to extension modules
# via resource://appdashboard/modules/[module name] URLs.
resource appdashboard ./

content appdashboard content/

# localizations in alphabetical order
locale  appdashboard   en-US   locale/en-US/

overlay chrome://browser/content/browser.xul           chrome://appdashboard/content/browser.xul
