{
  "title": "Dictionary Notifications",
  "entries": [
{% set first = True; %}
{% for result in results %}{% if not first %},{% end %}{% set first = False %}
    {
      "title":{{ encode(result["title"]) }},
      "link":{{ encode(result["link"]) }},
      "id":{{ encode(result["id"]) }},
      "updated":"{{ result["updated"] }}",
      "summary":{{ encode(result["summary"]) }}}{% end %}
  ]
}