{
  "title": "{{ title }}",
  "results":[
{% for result in results %}
    {"title":{{ encode(result.title) }},
{% if result.category %}     "category_term":"{{ result.category }}",
{% end %}     "link":{{ encode(result.link) }},
{% if result.updated %}     "updated":"{{ result.updated }}",
{% end %}     "summary":{{ encode(result.summary) }}},
{% end %}
  ]
}